{
	"name": "@mug-space/mug-server",
	"version": "1.0.0",
	"description": "Mug Space Server",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build": "yarn clean && nest build",
		"start": "AWS_PROFILE=mugspace nest start --watch",
		"dev": "AWS_PROFILE=mugspace nest start --debug --watch",
		"clean": "rimraf dist",
		"deploy": "sls deploy --aws-profile mugspace --stage prod",
		"typeorm": "typeorm-ts-node-commonjs",
		"migration:generate": "AWS_PROFILE=mugspace yarn run typeorm migration:generate src/migrations/migrations --dataSource src/ormconfig.ts",
		"migration:run": "AWS_PROFILE=mugspace yarn run typeorm migration:run -d src/ormconfig.ts",
		"migration:revert": "AWS_PROFILE=mugspace yarn run typeorm migration:revert -d src/ormconfig.ts"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "3.511.0",
		"@aws-sdk/client-secrets-manager": "^3.298.0",
		"@aws-sdk/lib-storage": "^3.529.1",
		"@nestjs/axios": "^3.0.1",
		"@nestjs/common": "^10.3.1",
		"@nestjs/config": "^3.1.1",
		"@nestjs/core": "^10.3.1",
		"@nestjs/jwt": "^10.2.0",
		"@nestjs/passport": "^10.0.3",
		"@nestjs/platform-express": "^10.3.1",
		"@nestjs/swagger": "^7.2.0",
		"@nestjs/typeorm": "^10.0.2",
		"aws-lambda": "^1.0.7",
		"axios": "^0.27.2",
		"builder-pattern": "^1.3.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dayjs": "^1.11.9",
		"express": "^4.18.2",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.21",
		"mysql2": "^3.9.1",
		"passport": "^0.5.3",
		"passport-jwt": "^4.0.0",
		"passport-local": "^1.0.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.5.5",
		"serverless": "^3.38.0",
		"serverless-http": "^3.2.0",
		"serverless-prune-plugin": "^2.0.2",
		"swagger-ui-express": "4.1.4",
		"typeorm": "^0.3.20",
		"typeorm-transactional": "^0.5.0",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@nestjs/cli": "^8.2.6",
		"@nestjs/schematics": "^10.0.0",
		"@types/aws-lambda": "^8.10.114",
		"@types/compression": "^1",
		"@types/cookie-parser": "^1.4.3",
		"@types/cookie-signature": "^1.0.4",
		"@types/cors": "^2",
		"@types/express": "^4.17.13",
		"@types/ioredis": "^4.28.10",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/lodash": "^4.14.182",
		"@types/node": "^16.11.36",
		"@types/passport": "^1.0.7",
		"@types/passport-jwt": "^3.0.7",
		"@types/passport-local": "^1.0.34",
		"@types/rimraf": "^3.0.2",
